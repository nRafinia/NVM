@page "/"
@using Microsoft.AspNetCore.Components.Web
@namespace Agent.UI.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en" class="dark-style layout-menu-fixed" dir="ltr" data-theme="theme-default" data-assets-path="../assets/">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0"/>
    <base href="~/"/>
    <link rel="icon" type="image/png" href="favicon.png"/>

    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com"/>
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin/>
    <link href="https://fonts.googleapis.com/css2?family=Public+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet"/>

    <!-- Core CSS -->
    <link rel="stylesheet" href="/css/core-dark.css" class="template-customizer-core-css"/>
    <link rel="stylesheet" href="/css/theme-default-dark.css" class="template-customizer-theme-css"/>
    <link rel="stylesheet" href="/css/site.css"/>

    <!-- Vendors CSS -->
    <link rel="stylesheet" href="libs/perfect-scrollbar/perfect-scrollbar.css"/>

    <!-- Helpers -->
    <script src="js/helpers.js"></script>

    <script src="js/config.js"></script>

    <link href="Agent.UI.styles.css" rel="stylesheet"/>
    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered"/>
</head>
<body>
<component type="typeof(App)" render-mode="ServerPrerendered"/>

<div id="blazor-error-ui">
    <environment include="Staging,Production">
        An error has occurred. This application may no longer respond until reloaded.
    </environment>
    <environment include="Development">
        An unhandled exception has occurred. See browser dev tools for details.
    </environment>
    <a href="" class="reload">Reload</a>
    <a class="dismiss">🗙</a>
</div>

<!-- Core JS -->
<script src="/libs/jquery/jquery.js"></script>
<script src="/libs/popper/popper.js"></script>
<script src="/js/bootstrap.js"></script>
<script src="/libs/perfect-scrollbar/perfect-scrollbar.js"></script>

<script src="/js/menu.js"></script>
<!-- Main JS -->
<script src="/js/main.js"></script>

<script src="_framework/blazor.server.js"></script>

<script async defer src="https://buttons.github.io/buttons.js"></script>
</body>
</html>